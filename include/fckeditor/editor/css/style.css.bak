@charset "utf-8";
/* CSS Document */

body
	{
		/*background:url(../images/body-bg.jpg) repeat-x #3a87ca;*/
		margin:20px 0 0 0px;
		padding:0px;
		font-family:Arial, Helvetica, sans-serif;
		font-size:12px;
		color:#000000;
		text-align:justify;
	}
	
form
	{
		margin:0px;
		padding:0px;
	}
	
img
	{
		border:none;
	}
	
a
	{
		outline:none;
	}
	
p,ol,ul,li
	{
		margin:0px;
		padding:0px;
		font-family:Arial, Helvetica, sans-serif;
		font-size:12px;
		color:#000000;
	}
	
/*---------------header css-----------*/
	
.header-bg-lt
	{
		background:url(../images/header-bg-lt.jpg) no-repeat;
	}
	
.header-bg-rt
	{
		background:url(../images/header-bg-rt.jpg) no-repeat;
	}
	
.top-link
	{
		font-size:10px;
		color:#FFFFFF;
		text-decoration:none;
	}
	
.top-link:hover
	{
		text-decoration:underline;
	}
	
.call-today
	{
		font-size:19px;
		color:#FFFFFF;
		font-weight:bold;
	}
	
.search-bg
	{
		background:url(../images/search-bg.png) no-repeat;
	}
	
.search-txt
	{
		font-size:11px;
		color:#89d4ff;
	}
	
.input
	{
		width:135px;
		border:1px solid #99d8fc;
	}
	
.nutri
	{
		background:url(../images/nutri-bg.png) no-repeat;
		font-size:10px;
		color:#008dcd;
	}

.small-txt
	{
		font-size:10px;
		color:#5d5d5d;
	}
	
.links-bg
	{
		background:url(../images/links-bg.gif) repeat-x;
		height:40px;
	}
	
.main-links ul
	{
		margin:0px;
		padding:0px;
		float:left;
		
	}
	
.main-links ul li
	{
		margin:0px 0px 0 0px;
		list-style:none;
		background:url(../images/spacer.gif) no-repeat left;
		float:left;
		display:block;
		overflow:hidden;
		
		
	}
	
.main-links ul li a
	{
		font-family:Arial, Helvetica, sans-serif;
		color:#ffffff;
		font-size:12px;
		font-weight:bold;
		line-height:18px;
		text-transform:uppercase;
		text-decoration:none;
		padding:10px 20px 0 20px;
		height:30px;
		margin:0 0px;
		display:block;
		float:left;
		
	}
	
.main-links ul li a:hover
	{
		background:url(../images/link-hover.gif) repeat-x;
		text-decoration:underline;
		
	}
	
.shopping-cart
	{
		width:225px;
		height:40px;
		float:right;
		font-size:11px;
		color:#FFFFFF;
	}
.shopping-cart a
	{
		font-size:11px;
		color:#FFFFFF;
	}
	
.shopping-cart a:hover
	{
		font-size:11px;
		color:#89d4ff;
	}
	
/*---------------header css-----------*/

/*---------------body css-----------*/
.body-bg
	{
		background:url(../images/bdy-bg.gif) repeat-x #FFFFFF;
		border:1px solid #FFFFFF;
	}
/*---------------left panel css-----------*/	
.lt-link-bg
	{
		background:url(../images/left-nav-bg.gif) repeat-x top #228ce1;
		border:1px solid #c7c7c7;
	}
	
.box-heading
	{
		font-size:13px;
		color:#8bd3ff;
	}
	
.lt-links ul
	{
		margin:1px;
		padding:0px;
		float:left;
		
	}
	
.lt-links ul li
	{
		
		list-style:none;
		background:url(../images/arrow-dot.gif) no-repeat 5px 0px;
		display:block;
		
	}
	
.lt-links ul li a
	{
		width:173px;
		height:20px;
		font-family:Arial, Helvetica, sans-serif;
		color:#FFFFFF;
		font-size:12px;
		line-height:18px;
		text-decoration:none;
		margin:0px;
		padding:3px 0 0 25px;
		display:block;
	}
	
.lt-links ul li a:hover
	{
		color:#FFFFFF;
		text-decoration:none;
		background:#001439;
	}
	
	
ul.menu li 
	{
		float:left;
		margin-right:-1px;
		position:relative;
		z-index:99;
	}
	
ul.menu ul 
	{
		display:none;
		left:197px;
		list-style:none;
		opacity:99;
		position:absolute;
		top:0px;
		width:180px;
		margin:0px;
	}
ul.menu ul li 
	{
		
		margin:0;
		position:relative;
		height:22px;
		text-align:left;
		background-image:none;
		
	}
ul.menu ul li a 
	{
		border:1px solid #c7c7c7;
		.border-top:none;
		background-color:#228ce1;
		color:#ffffff;
		display:block;
		font-family:Arial,Helvetica,sans-serif;
		font-size:11px;
		height:22px;

	}
	
ul.menu ul li a:hover 
	{
		background-color:#001439;
	}
	
	
	

	
