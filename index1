    	$BillsLeft=(($type=='B')?$pendingBills.' Bills':$objMisc->dateFormat($objMisc->GiveValue("HEADOFFICE_ID='$_SESSION[HEADOFFICE]'",'EXPIRY_DATE','headoffice_master')));
